<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#ffffff"/>
            <corners android:radius="818dp" />
            <stroke
                android:width="2dp"
                android:color="#949494"
                />
            <gradient
                android:type="linear"
                android:startColor="#D5A7AB"
                android:centerColor="#8D9195"
                android:endColor="#C0E1F3"
                android:angle="0"
                />
        </shape>
    </item>
</selector>